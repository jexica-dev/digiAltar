dleLogout={props.handleLogout} user={props.user} images={images}  altars={altars} />\n        </Route>\n        <Route path=\"/myaltars/:altarId/edit\">\n          <AltarEdit user={props.user} images={images}  altars={altars} />\n        </Route>\n        <Route path=\"/about\">\n          <About />\n        </Route>\n        <Route path=\"/\" exact>\n        <DragExample />\n        </Route>\n      </Switch>\n    </div>\n  );\n}\n","/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/services/altars.js",[],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/services/images.js",[],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/components/LogoutBar/LogoutBar.jsx",[],["154","155"],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/components/AltarCard/AltarCard.jsx",[],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/components/AltarImage/AltarImage.jsx",[],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/screens/AltarEdit/AltarEdit.jsx",["156"],"import React from 'react'\nimport { useParams } from 'react-router';\nimport AltarForm from '../../components/AltarForm/AltarForm';\n\n  \nexport default function AltarEdit(props) {\n  const { altarId } = useParams();\n  return (\n    <div>\n      {props.altars.filter((altar) => {\n        return (altar.id == altarId);\n      }).map((altar) => {\n        return (\n          <AltarForm user={props.user} images={props.images}  altar={altar} />\n        )\n      })}\n  \n      \n    </div>\n  )\n}\n","/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/components/AltarForm/AltarForm.jsx",["157"],"/Users/jexica/Sync/general_assembly/Chickadee/unit_4/week_12/DigiAltar/client/src/components/DragMain/DragExample.jsx",["158"],"import { useState, useCallback } from 'react';\nimport { DragContainer } from './Drag